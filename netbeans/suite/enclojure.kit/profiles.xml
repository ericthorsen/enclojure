<?xml version="1.0" encoding="UTF-8"?>
<!--
    Project-local overrides to the build process based on detected or user-provided environmental parameters.
-->
<profilesXml xmlns="http://maven.apache.org/PROFILES/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/PROFILES/1.0.0 http://maven.apache.org/xsd/profiles-1.0.0.xsd">
   <profiles>
       <profile>
           <id>netbeans-ide</id>
           <properties>
               <netbeans.installation>path-to-associated-netbeans-install</netbeans.installation>
           </properties>
       </profile>
   </profiles>
   
   <activeProfiles>
       <activeProfile>netbeans-ide</activeProfile>
   </activeProfiles>
</profilesXml>
